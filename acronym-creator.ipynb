    "def acronym(sentence):\n",
    "    acr = \"\"\n",
    "    i = sentence.split()\n",
    "    for i in i:\n",
    "        acr += i[0].upper()\n",
    "    return acr\n",
    "user_input = \"Creating a Sentence for Acronym\"\n",
    "output = acronym(user_input)\n",
    "print(output)"
   ]
